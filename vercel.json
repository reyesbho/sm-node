{ 
    "version": 2, 
    "builds": [
        {
            "src": "server-with-local.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "src/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server-with-local.js"
        }
    ]
}
